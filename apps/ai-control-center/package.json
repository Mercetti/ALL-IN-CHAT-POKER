{
  "name": "ai-control-center",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "main": "electron/main.js",
  "scripts": {
    "dev": "concurrently -k \"npm:dev:renderer\" \"npm:dev:electron\"",
    "dev:renderer": "vite",
    "dev:electron": "wait-on http://localhost:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron electron/main.js",
    "build": "vite build",
    "start": "cross-env NODE_ENV=production electron electron/main.js",
    "package": "npm run build && electron-builder --config electron-builder.config.json"
  },
  "author": "Mercetti",
  "dependencies": {
    "axios": "^1.7.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.8.4",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.2",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "electron": "^32.1.2",
    "electron-builder": "^25.0.5",
    "typescript": "^5.6.3",
    "vite": "^5.4.8",
    "wait-on": "^7.2.0"
  }
}
