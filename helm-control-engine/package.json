{
  "name": "helm-control-engine",
  "version": "2.0.0",
  "description": "Full Helm Control Engine with TypeScript and Expo mobile wiring",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "mobile": "expo start",
    "mobile:android": "expo start --android",
    "mobile:ios": "expo start --ios",
    "mobile:build": "expo build",
    "clean": "rimraf dist"
  },
  "keywords": [
    "ai",
    "orchestration",
    "skills",
    "mobile",
    "typescript",
    "expo"
  ],
  "author": "Helm Control Team",
  "license": "MIT",
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.6",
    "expo": "~49.0.15",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.3.20",
    "expo-status-bar": "~1.6.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-reanimated": "~3.3.0",
    "axios": "^1.6.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "ws": "^8.14.2",
    "node-cron": "^3.0.3",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "joi": "^17.11.0",
    "nodemailer": "^6.9.7"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/express": "^4.17.20",
    "@types/cors": "^2.8.15",
    "@types/morgan": "^1.9.7",
    "@types/ws": "^8.5.8",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/bcryptjs": "^2.4.5",
    "@types/nodemailer": "^6.4.13",
    "@types/react": "^18.2.37",
    "@types/react-native": "^0.72.6",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.6",
    "ts-jest": "^29.1.1",
    "eslint": "^8.52.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "rimraf": "^5.0.5",
    "@expo/webpack-config": "^18.0.1",
    "webpack": "^5.88.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "expo": {
    "name": "Helm Control Engine",
    "slug": "helm-control-engine",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.helm.control"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.helm.control"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "webpack"
    }
  }
}
