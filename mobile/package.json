{
  "name": "all-in-chat-poker-mobile",
  "version": "1.0.0",
  "description": "All-In Chat Poker - Native Mobile App",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --watch --config jest.config.js",
    "test:coverage": "jest --coverage --config jest.config.js",
    "test:performance": "jest --testNamePattern=performance --config jest.config.js",
    "test:ci": "npm run test && npm run test:coverage && npm run test:performance",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "test:full": "bash scripts/test.sh",
    "clean": "react-native clean",
    "pod-install": "cd ios && pod install",
    "metro": "npx react-native start --reset-cache",
    "build:android": "cd android && ./gradlew assembleDebug",
    "build:ios": "cd ios && xcodebuild -project AllInChatPoker.xcworkspace -scheme AllInChatPoker -configuration Debug -destination generic/platform=iOS Simulator,name=iPhone 15",
    "run:all": "concurrently \"npm run start\" \"npm run test:watch\""
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.6",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "react-native-screens": "^3.29.0",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-vector-icons": "^10.0.2",
    "react-native-gesture-handler": "^2.14.1",
    "react-native-reanimated": "^3.6.2",
    "react-native-svg": "^14.1.0",
    "react-native-linear-gradient": "^2.8.3",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "react-native-webview": "^13.8.1",
    "react-native-permissions": "^3.10.0",
    "react-native-device-info": "^10.11.0",
    "react-native-share": "^9.4.1",
    "react-native-sound": "^0.11.2",
    "react-native-push-notification": "^8.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.12",
    "@babel/runtime": "^7.20.12",
    "metro-react-native-babel-preset": "^0.77.0",
    "jest": "^29.7.0",
    "react-test-renderer": "^18.2.0",
    "@testing-library/react-native": "^12.4.2",
    "eslint": "^8.57.0",
    "eslint-config-react-native": "^4.1.0",
    "prettier": "^3.2.5"
  },
  "peerDependencies": {
    "react": "18.2.0"
  },
  "keywords": [
    "poker",
    "mobile",
    "react-native",
    "gaming",
    "streaming",
    "twitch",
    "blackjack",
    "tournaments"
  ],
  "author": "Mercetti",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/merce/all-in-chat-poker"
  },
  "bugs": {
    "url": "https://github.com/merce/all-in-chat-poker/issues"
  },
  "homepage": "https://all-in-chat-poker.com"
}
