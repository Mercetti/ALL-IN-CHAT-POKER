{
  "expo": {
    "name": "Helm Control Dashboard",
    "slug": "helm-control-dashboard",
    "version": "1.0.0",
    "sdkVersion": "54.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#121212"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mercetti.acey-control-center"
    },
    "android": {
      "package": "com.mercetti.acey_control_center",
      "versionCode": 1,
      "permissions": [
        "INTERNET"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3b8d3730-f772-41b6-b259-26d52e3889fc"
      }
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
