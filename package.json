{
  "name": "poker-game-overlay",
  "version": "1.0.0",
  "description": "Interactive video poker game overlay for Twitch streams",
  "type": "commonjs",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "cross-env NODE_ENV=development node server.js",
    "prod": "cross-env NODE_ENV=production node server.js",
    "test": "cross-env NODE_ENV=test node --test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "security:audit": "npm audit --audit-level high",
    "security:check": "node -e \"const validator = require('./server/config-validator'); validator.printValidationReport();\"",
    "deploy": "chmod +x deploy.sh && ./deploy.sh",
    "deploy:rollback": "./deploy.sh --rollback",
    "health": "curl -f http://localhost:3000/health || exit 1",
    "logs": "tail -f logs/app.log",
    "backup": "cp data/data.db backups/data_$(date +%Y%m%d_%H%M%S).db",
    "desktop": "electron electron/main.js",
    "desktop:pack": "electron-packager . allin-chat-bot --platform=win32 --arch=x64 --out=dist --overwrite --prune=false --ignore=\"^/dist$|^/.git$|^/test$\""
  },
  "keywords": [
    "poker",
    "twitch",
    "overlay",
    "game",
    "video-poker"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^9.0.0",
    "csurf": "^1.11.0",
    "diff": "^5.2.0",
    "discord.js": "^14.25.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jpeg-js": "^0.4.4",
    "jsonwebtoken": "^9.0.2",
    "pg": "8.11.5",
    "pngjs": "^7.0.0",
    "socket.io": "^4.6.1",
    "tmi.js": "^1.8.5",
    "winston": "^3.11.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "electron": "^39.2.7",
    "electron-packager": "^17.1.2",
    "eslint": "^9.39.2",
    "globals": "^15.13.0",
    "htmlhint": "^1.8.0",
    "markdownlint-cli": "^0.47.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.7.4",
    "stylelint": "^16.26.1",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
